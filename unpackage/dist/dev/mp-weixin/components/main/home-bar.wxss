.main-home-bar {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
}
.main-home-bar .tab-bar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background-color: #373737;
  padding: 12rpx 0;
}
.main-home-bar .tab-bar .bar-btn {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  text-align: center;
}
.main-home-bar .tab-bar .bar-btn .bar-view {
  width: 70rpx;
  height: 50rpx;
  position: relative;
  margin: auto;
  margin-bottom: 8rpx;
}
.main-home-bar .tab-bar .bar-btn .img-bar {
  width: 70rpx;
  height: 50rpx;
}
.main-home-bar .tab-bar .bar-btn .txt {
  color: #fff;
  font-size: 28rpx;
}
.main-home-bar .tab-bar .bar-btn.active .txt {
  color: #bfa56d;
  font-size: 24rpx;
}
.main-home-bar .tab-bar .bar-btn.active .img-bar {
  width: 70rpx;
  height: 74rpx;
  position: absolute;
  bottom: 0;
  left: 0;
}
.main-home-bar .tab-bar .bar-btn.active .img-bar.img-person {
  width: 102rpx;
  height: 74rpx;
  left: -10rpx;
}

