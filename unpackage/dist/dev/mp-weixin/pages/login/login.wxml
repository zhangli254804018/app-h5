<view class="content login-content"><view class="login-form"><image class="logo" src="../../static/img/login/logo-L.png"></image><view class="form-content"><view class="form-item form-account"><image class="item-icon" src="../../static/img/login/cell-phone-number.png"></image><input type="text" value="{{queryForm.account}}" placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['$0','account','$event',['trim']],['queryForm']]]],['blur',[['$forceUpdate']]]]}}" bindinput="__e" bindblur="__e"/></view><view class="form-item form-password"><image class="item-icon" src="../../static/img/login/password.png"></image><input type="password" value="{{queryForm.passwd}}" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','passwd','$event',['trim']],['queryForm']]]],['blur',[['$forceUpdate']]]]}}" bindinput="__e" bindblur="__e"/></view><view class="form-item form-captcha"><image class="item-icon" src="../../static/img/login/Verification-Code.png"></image><input type="text" value="{{queryForm.captcha}}" placeholder="请输入图形验证码" data-event-opts="{{[['input',[['__set_model',['$0','captcha','$event',['trim']],['queryForm']]]],['blur',[['$forceUpdate']]]]}}" bindinput="__e" bindblur="__e"/><image class="captcha" src="{{getCaptcha.img}}" mode="scaleToFill" data-event-opts="{{[['tap',[['initData',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="form-submit"><view data-event-opts="{{[['tap',[['onSubmit',['$event']]]]]}}" class="submnit-btn" bindtap="__e">登陆</view></view><view class="register-tip"><view data-event-opts="{{[['tap',[['toReg',['$event']]]]]}}" class="btn-login" bindtap="__e">注册账户</view></view><view class="version">版本1.0.17</view></view></view>